{
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:tailwind": "concurrently \"next dev\" \"npm run tailwind -- --watch --minify\"",
    "dev:tailwindturbo": "concurrently \"next dev --turbo\" \"npm run tailwind -- --watch --minify\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i styles/globals.scss -o styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "npm run tailwind"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.0",
    "axios": "1.2.6",
    "classnames": "2.3.2",
    "concurrently": "7.5.0",
    "follow-redirects": "1.15.2",
    "formik": "2.2.9",
    "nanoid": "4.0.0",
    "next": "13.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.6.0",
    "react-redux": "8.0.5",
    "react-toastify": "9.1.1"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/webpack": "5.28.0",
    "autoprefixer": "10.4.13",
    "postcss-import": "15.1.0",
    "postcss-nesting": "10.2.0",
    "sass": "1.56.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  }
}
